@model Artist
@{ 
    ViewBag.Title = "Delete Artist";
}
<h2>confirm Delete?</h2>
<h3>@Model.ArtistName</h3>
<form asp-action="Delete" method="post">
    <input type="hidden" asp-for="ArtistId"/>
    <button type="submit" class="btn btn-primary p-2">Delete</button>
    <a asp-controller="Artist" asp-action="Index" class="btn btn-primary p-2">Cancel</a>
</form>
